{
  "multipart": [
    {
      "when": {
        "axis_along_first": "false",
        "facing": "down"
      },
      "apply": {
        "model": "create:block/deployer/horizontal",
        "x": 270,
        "y": 90
      }
    },
    {
      "when": {
        "axis_along_first": "false",
        "facing": "east"
      },
      "apply": {
        "model": "create:block/deployer/horizontal",
        "y": 270
      }
    },
    {
      "when": {
        "axis_along_first": "false",
        "facing": "north"
      },
      "apply": {
        "model": "create:block/deployer/vertical",
        "y": 180
      }
    },
    {
      "when": {
        "axis_along_first": "false",
        "facing": "south"
      },
      "apply": {
        "model": "create:block/deployer/vertical"
      }
    },
    {
      "when": {
        "axis_along_first": "false",
        "facing": "up"
      },
      "apply": {
        "model": "create:block/deployer/horizontal",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "axis_along_first": "false",
        "facing": "west"
      },
      "apply": {
        "model": "create:block/deployer/horizontal",
        "y": 90
      }
    },
    {
      "when": {
        "axis_along_first": "true",
        "facing": "down"
      },
      "apply": {
        "model": "create:block/deployer/horizontal",
        "x": 270
      }
    },
    {
      "when": {
        "axis_along_first": "true",
        "facing": "east"
      },
      "apply": {
        "model": "create:block/deployer/vertical",
        "y": 270
      }
    },
    {
      "when": {
        "axis_along_first": "true",
        "facing": "north"
      },
      "apply": {
        "model": "create:block/deployer/horizontal",
        "y": 180
      }
    },
    {
      "when": {
        "axis_along_first": "true",
        "facing": "south"
      },
      "apply": {
        "model": "create:block/deployer/horizontal"
      }
    },
    {
      "when": {
        "axis_along_first": "true",
        "facing": "up"
      },
      "apply": {
        "model": "create:block/deployer/horizontal",
        "x": 90
      }
    },
    {
      "when": {
        "axis_along_first": "true",
        "facing": "west"
      },
      "apply": {
        "model": "create:block/deployer/vertical",
        "y": 90
      }
    },
    {
      "when": {"OR": [
        {"facing": "north", "axis_along_first": false},
        {"facing": "east", "axis_along_first": true},
        {"facing": "south","axis_along_first": false},
        {"facing": "west", "axis_along_first": true}
      ]},
      "apply": {
        "model": "create:block/shaft"
      }
    },
    {
      "when": {"AND": [
        {"axis_along_first": true},
        {"OR": [
          {"facing": "up"},
          {"facing": "down"},
          {"facing": "north"},
          {"facing": "south"}
        ]}
      ]},
      "apply": {
        "model": "create:block/shaft",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {"AND": [
        {"axis_along_first": false},
        {"OR": [
          {"facing": "up"},
          {"facing": "down"},
          {"facing": "east"},
          {"facing": "west"}
        ]}
      ]},
      "apply": {
        "model": "create:block/shaft",
        "x": 90
      }
    },
    {
      "when": {"facing": "north"},
      "apply": {
        "model": "create:block/deployer/pole",
        "y": 180
      }
    },
    {
      "when": {"facing": "north"},
      "apply": {
        "model": "create:block/deployer/hand_pointing",
        "y": 180
      }
    },
    {
      "when": {"facing": "east"},
      "apply": {
        "model": "create:block/deployer/pole",
        "y": 270
      }
    },
    {
      "when": {"facing": "east"},
      "apply": {
        "model": "create:block/deployer/hand_pointing",
        "y": 270
      }
    },
    {
      "when": {"facing": "south"},
      "apply": {
        "model": "create:block/deployer/pole"
      }
    },
    {
      "when": {"facing": "south"},
      "apply": {
        "model": "create:block/deployer/hand_pointing"
      }
    },
    {
      "when": {"facing": "west"},
      "apply": {
        "model": "create:block/deployer/pole",
        "y": 90
      }
    },
    {
      "when": {"facing": "west"},
      "apply": {
        "model": "create:block/deployer/hand_pointing",
        "y": 90
      }
    },
    {
      "when": {"facing": "up"},
      "apply": {
        "model": "create:block/deployer/pole",
        "x": 90
      }
    },
    {
      "when": {"facing": "up"},
      "apply": {
        "model": "create:block/deployer/hand_pointing",
        "x": 90
      }
    },
    {
      "when": {"facing": "down"},
      "apply": {
        "model": "create:block/deployer/pole",
        "x": 270
      }
    },
    {
      "when": {"facing": "down"},
      "apply": {
        "model": "create:block/deployer/hand_pointing",
        "x": 270
      }
    }
  ]
}