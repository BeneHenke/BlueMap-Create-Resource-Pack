{
  "multipart": [
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "create:block/fluid_pipe/window",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "axis": "y"
      },
      "apply": {
        "model": "create:block/fluid_pipe/window"
      }
    },
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "create:block/fluid_pipe/window",
        "x": 90
      }
    },
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "create:block/fluid_pipe/rim/east"
      }
    },
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "create:block/fluid_pipe/rim/west"
      }
    },
    {
      "when": {
        "axis": "y"
      },
      "apply": {
        "model": "create:block/fluid_pipe/rim/up"
      }
    },
    {
      "when": {
        "axis": "y"
      },
      "apply": {
        "model": "create:block/fluid_pipe/rim/down"
      }
    },
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "create:block/fluid_pipe/rim/north"
      }
    },
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "create:block/fluid_pipe/rim/south"
      }
    }
  ]
}