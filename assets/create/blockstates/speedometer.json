      },
      "apply": {
        "model": "create:block/gauge/base",
        "y": 90
      }
    },
    {
      "when": {
        "axis_along_first": "true",
        "facing": "west"
      },
      "apply": {
        "model": "create:block/gauge/base_wall",
        "x": 90,
        "y": 180
      }
    },
    {
      "when": {
        "axis_along_first": false
      },
      "apply": {
        "model": "create:block/shaft",
        "x": 90
      }
    },
    {
      "when": {
        "axis_along_first": true
      },
      "apply": {
        "model": "create:block/shaft",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "create:block/gauge/speedometer/head",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "create:block/gauge/speedometer/head"
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "create:block/gauge/speedometer/head",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "create:block/gauge/speedometer/head",
        "y": 180
      }
    },
    {
      "when": {
        "OR": [
          {
            "facing": "up"
          },
          {
            "facing": "down"
          }
        ],
        "axis_along_first": "true"
      },
      "apply": {
        "model": "create:block/gauge/speedometer/head",
        "y": 270
      }
    },
    {
      "when": {
        "OR": [
          {
            "facing": "up"
          },
          {
            "facing": "down"
          }
        ],
        "axis_along_first": "true"
      },
      "apply": {
        "model": "create:block/gauge/speedometer/head",
        "y": 90
      }
    },
    {
      "when": {
        "OR": [
          {
            "facing": "up"
          },
          {
            "facing": "down"
          }
        ],
        "axis_along_first": "false"
      },
      "apply": {
        "model": "create:block/gauge/speedometer/head"
      }
    },
    {
      "when": {
        "OR": [
          {
            "facing": "up"
          },
          {
            "facing": "down"
          }
        ],
        "axis_along_first": "false"
      },
      "apply": {
        "model": "create:block/gauge/speedometer/head",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "create:block/gauge/dial",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "create:block/gauge/dial"
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "create:block/gauge/dial",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "create:block/gauge/dial",
        "y": 180
      }
    },
    {
      "when": {
        "OR": [
          {
            "facing": "up"
          },
          {
            "facing": "down"
          }
        ],
        "axis_along_first": "true"
      },
      "apply": {
        "model": "create:block/gauge/dial",
        "y": 270
      }
    },
    {
      "when": {
        "OR": [
          {
            "facing": "up"
          },
          {
            "facing": "down"
          }
        ],
        "axis_along_first": "true"
      },
      "apply": {
        "model": "create:block/gauge/dial",
        "y": 90
      }
    },
    {
      "when": {
        "OR": [
          {
            "facing": "up"
          },
          {
            "facing": "down"
          }
        ],
        "axis_along_first": "false"
      },
      "apply": {
        "model": "create:block/gauge/dial"
      }
    },
    {
      "when": {
        "OR": [
          {
            "facing": "up"
          },
          {
            "facing": "down"
          }
        ],
        "axis_along_first": "false"
      },
      "apply": {
        "model": "create:block/gauge/dial",
        "y": 180
      }
    }
  ]
